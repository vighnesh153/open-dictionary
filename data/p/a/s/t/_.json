{
  "word": "past",
  "etymologies": [
    {
      "description": [
        "From Middle English, past participle of passen (“to pass, to go by”), whence Modern English pass."
      ],
      "adjectives": [
        {
          "description": "past (comparative more past, superlative most past)",
          "definitionGroups": [
            {
              "group": null,
              "entries": [
                {
                  "meaning": "Having already happened; in the past; finished. [from 14th c.]",
                  "examples": [
                    "past glories"
                  ]
                }
              ]
            },
            {
              "group": null,
              "entries": [
                {
                  "meaning": "(postmodifier) Following expressions of time to indicate how long ago something happened; ago. [from 15th c.]",
                  "examples": [
                    "That had been, what, three years past?"
                  ]
                }
              ]
            },
            {
              "group": null,
              "entries": [
                {
                  "meaning": "Of a period of time: having just gone by; previous. [from 15th c.]",
                  "examples": [
